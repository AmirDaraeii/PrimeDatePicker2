<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="@dimen/material_size_2"
        android:layout_marginEnd="@dimen/material_size_2"
        android:background="@drawable/range_background_selected"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.aminography.primedatepicker.picker.header.TwoLineTextView
        android:id="@+id/twoLineTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingStart="@dimen/material_size_8"
        android:paddingTop="@dimen/material_size_4"
        android:paddingEnd="@dimen/material_size_8"
        android:paddingBottom="@dimen/material_size_4"
        app:firstLabelTextColor="@color/white"
        app:firstLabelTextSize="@dimen/text_size_header_multi_large"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:secondLabelTextColor="@color/white"
        app:secondLabelTextSize="@dimen/text_size_header_multi_small" />

</androidx.constraintlayout.widget.ConstraintLayout>